<odoo>
    <record id="employees_statistics_line_view_tree" model="ir.ui.view">
        <field name="name">Employees Statistics List</field>
        <field name="model">employees.statistics.line</field>

        <field name="arch" type="xml">
            <tree expand="True" editable="bottom">
                <field name="name" invisible="True"/>
                <field name="job_position" invisible="True"/>
                <field name="date"/>
                <field name="benchmark"/>
                <field name="value"/>
                <field name="week" invisible="True"/>
            </tree>
        </field>
    </record>
</odoo>