        <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
    </record>

    <record id="group_accountant" model="res.groups">
        <field name="name">Amazon Accountant</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
    </record>
</odoo>