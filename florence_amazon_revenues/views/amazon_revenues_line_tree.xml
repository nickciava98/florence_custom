<odoo>
    <record id="amazon_revenues_line_view_tree" model="ir.ui.view">
        <field name="name">Revenues Tree</field>
        <field name="model">amazon.revenues.line</field>

        <field name="arch" type="xml">
            <tree expand="True" editable="bottom">
                <field name="date"/>
                <field name="price_unit" string="Selling Price"/>
                <field name="amazon_fees"/>
                <field name="taxes" string="VAT"/>
                <field name="sku_cost"/>
                <field name="gross_revenues"/>
                <field name="ads_total_cost"/>
                <field name="ads_cost_per_unit"/>
                <field name="pcs_sold"/>
                <field name="earned_per_pc"/>
                <field name="probable_income"/>
            </tree>
        </field>
    </record>
</odoo>