<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- scss & JS Assets -->
    <template id="theme_assets_frontend" name="Product Carousel Frontend Assets"
              inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <link href="/theme_florence/static/src/scss/header_template.scss"
                  rel="stylesheet" type="text/scss"/>
            <link href="/theme_florence/static/src/scss/footer.scss"
                  rel="stylesheet" type="text/scss"/>
            <link href="/theme_florence/static/src/scss/homepage.scss"
                  rel="stylesheet" type="text/scss"/>
            <link href="/theme_florence/static/src/scss/jquery.fancybox.min.css"
                  rel="stylesheet" type="text/scss"/>
            <link href="/theme_florence/static/src/scss/faq.scss"
                  rel="stylesheet" type="text/scss"/>
            <link href="/theme_florence/static/src/scss/single_product_template.scss"
                  rel="stylesheet" type="text/scss"/>
            <link href="/theme_florence/static/src/scss/caveat_red_hat.css"
                  rel="stylesheet" type="text/scss"/>
            <link href="/theme_florence/static/src/scss/bootstrap-side-modals.css"
                  rel="stylesheet" type="text/scss"/>
            <!--<link href="https://fonts.googleapis.com/css2?family=Caveat:wght@400;500;600;700&amp;display=swap"
                  rel="stylesheet"/>
            <link href="https://fonts.googleapis.com/css2?family=Caveat:wght@400;500;600;700&amp;family=Red+Hat+Display:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;display=swap"
                  rel="stylesheet"/>-->
            <script type="text/javascript" src="/theme_florence/static/src/js/jquery.fancybox.min.js"/>
            <script type="text/javascript" src="/theme_florence/static/src/js/theme_common.js"/>
        </xpath>
    </template>
    <template id="snippet_product_carousel_place" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_feature']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="theme_florence.snippets_florence_bio_arctonik_250_ml"/>
            <t t-snippet="theme_florence.snippets_2"/>
            <t t-snippet="theme_florence.snippets_3"/>
            <t t-snippet="theme_florence.snippets_4"/>
            <t t-snippet="theme_florence.snippets_5"/>
        </xpath>
    </template>

</odoo>
