<odoo>
    <record id="send_stock_forecast_reminder_action" model="ir.cron">
        <field name="name">Send Stock Forecast Reminder</field>
        <field name="model_id" ref="florence_stock.model_forecast_configuration"/>
        <field name="state">code</field>
        <field name="code">model.send_stock_forecast_reminder_action()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">3</field>
        <field name="interval_type">months</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
    </record>
</odoo>